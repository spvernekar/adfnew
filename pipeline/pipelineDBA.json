{
	"name": "pipelineDBA",
	"properties": {
		"activities": [
			{
				"name": "CopyDBAdataActcivity",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "dbastoragekp",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sqldataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"dbname": {
				"type": "String",
				"defaultValue": "dba"
			},
			"landingfld": {
				"type": "String",
				"defaultValue": "landingfolder"
			}
		},
		"annotations": []
	}
}